@model ComicCorner.Models.Comic

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete @Model.ComicName</h2>
<form method="Post" action="/Comic/DeleteConfirm/@Model.ComicId">
    <input type="hidden" name="ComicId" value="@Model.ComicId" />
    <input type="submit" value="Yes" />
    <a href="/Comic/List">No, go back to list</a>
</form>

